{
    "manifest_version": 2,
    "background": { "page": "background.html" },
    "name": "SageAxcess",
    "version": "1.0.0",
    "description": "",
    "permissions": [ "<all_urls>", "tabs", "webRequest", "webRequestBlocking" ],
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "js": [ "contentscript.js" ],
            "all_frames": true,
            "run_at": "document_start"
        }
    ]
}